                    <div class="tags d-flex">
                        <span class="tag-title">Tags:</span>
                        <ul class="tag-list media-body">
                            <li><a href="#">drone,</a></li>
                            <li><a href="#">camera</a></li>
                        </ul>
                    </div>
                    <h2 class="product-title">@Model.Product.Title  @overal</h2>
                    <div class="review-rating d-flex flex-wrap">
                        <ul class="rating">
                            @for (int i = 1; i <= 5; i++)
                            {
                                if (i <= overal)
                                {
                                    <li class="rating-on"><i class="fas fa-star"></i></li>

                                }
                                else if (modul > 0)
                                {
                                    modul = 0;
                                    <li class="rating-on"><i class="fas fa-star-half"></i></li>
                                }
                                else
                                {
                                    <li class="rating-on"><i class="far fa-star"></i></li>
                                }
                            }
